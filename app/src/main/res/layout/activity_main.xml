<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".view.MainActivity"
    android:background="@drawable/acti">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/empresaId"
        android:background="@drawable/empresa"
        android:layout_alignTop="@+id/servicoId"
        android:layout_alignLeft="@+id/imageView"
        android:layout_alignStart="@+id/imageView" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/servicoId"
        android:background="@drawable/servico"
        android:layout_marginTop="41dp"
        android:layout_below="@+id/imageView"
        android:layout_alignRight="@+id/imageView"
        android:layout_alignEnd="@+id/imageView" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/clienteId"
        android:layout_marginTop="41dp"
        android:background="@drawable/clientes"
        android:layout_below="@+id/empresaId"
        android:layout_alignLeft="@+id/empresaId"
        android:layout_alignStart="@+id/empresaId" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/contatoId"
        android:layout_alignTop="@+id/clienteId"
        android:layout_alignRight="@+id/servicoId"
        android:layout_alignEnd="@+id/servicoId"
        android:background="@drawable/contato" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:background="@drawable/tela1" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:text="Sair"
        android:id="@+id/empresa1"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/contatoId"
        android:layout_alignEnd="@+id/contatoId"
        android:layout_alignLeft="@+id/clienteId"
        android:layout_alignStart="@+id/clienteId" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:id="@+id/cadastroId"
        android:layout_above="@+id/empresa1"
        android:layout_centerHorizontal="true"
        android:background="@drawable/cadastro" />

</RelativeLayout>
